triggers='CREATE DEFINER=`root`@`localhost` TRIGGER `tbyrpjkad` AFTER DELETE ON `bayar_pajak` FOR EACH ROW BEGIN\nupdate penjualan set id_byrpjk=null where id_byrpjk=old.id_byrpjk;\ndelete from keuangan where no_transaksi=old.id_byrpjk and keterangan=\'Pembayaran PPN\';\n\ndelete from jurnal where no_transaksi=old.id_byrpjk;\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `tbyrpjkai` AFTER INSERT ON `bayar_pajak` FOR EACH ROW BEGIN\ninsert into keuangan(Tanggal,Keterangan,pengeluaran,no_transaksi) values(new.tgl_byr,\'Pembayaran PPN\',new.jum_byr,new.id_byrpjk);\ninsert into jurnal values(\'\',new.tgl_byr,\'5.6\',\'Beban PPN\',new.jum_byr,0,new.id_byrpjk,\'\');\nif new.byrdari=\'Kas\' then\ninsert into jurnal values(\'\',new.tgl_byr,\'1.1\',\'  Kas\',0,new.jum_byr,new.id_byrpjk,\'\');\nelse\ninsert into jurnal values(\'\',new.tgl_byr,\'1.2\',\'  Bank\',0,new.jum_byr,new.id_byrpjk,\'\');\nend if;\nEND'
