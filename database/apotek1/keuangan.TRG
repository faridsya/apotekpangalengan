TYPE=TRIGGERS
triggers='CREATE DEFINER=`root`@`localhost` TRIGGER `tkeuanganad` AFTER DELETE ON `keuangan` FOR EACH ROW BEGIN\ndelete from jurnal where no_transaksi=old.no_transaksi;\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `tkeuanganai` AFTER INSERT ON `keuangan` FOR EACH ROW BEGIN\n\nif new.Jenis=\'Lain-lain\' and new.no_akun is not null then\ninsert into jurnal values(\'\',new.Tanggal,new.no_akun,new.keterangan,new.pengeluaran,0,new.no_transaksi,\'\');\ninsert into jurnal values(\'\',new.Tanggal,\'1.1\',\'   Kas\',0,new.pengeluaran,new.no_transaksi,\'\');\nelse\nif new.jenis=\'Modal\' then\ninsert into jurnal values(\'\',new.Tanggal,\'1.1\',\'Kas\',new.pemasukan,0,new.no_transaksi,\'\');\ninsert into jurnal values(\'\',new.Tanggal,\'3.0\',\'   Modal\',0,new.pemasukan,new.no_transaksi,\'\');\nelse  \nif new.jenis=\'Lain-lain\' and new.pemasukan>0 then\ninsert into jurnal values(\'\',new.Tanggal,\'1.1\',\'Kas\',new.pemasukan,0,new.no_transaksi,\'\');\ninsert into jurnal values(\'\',new.Tanggal,\'4.2\',concat(\'   \',new.keterangan),0,new.pemasukan,new.no_transaksi,\'\');\n\n\nend if;\nend if;\nend if;\nEND'
