TYPE=TRIGGERS
triggers='CREATE DEFINER=`root`@`localhost` TRIGGER `ttblbarangad` AFTER DELETE ON `tblbarang` FOR EACH ROW BEGIN\ndelete from satuan where kode_brg=old.kode_brg;\ndelete from stokgudang where kode_brg=old.kode_brg;\ndelete from histori_harga where kode_brg=old.kode_brg;\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `ttblbarangai` AFTER INSERT ON `tblbarang` FOR EACH ROW BEGIN\ninsert into satuan values(new.Kode_brg,new.`satuan`,1,\'Utama\',new.harga_jual);\ninsert into stokgudang values(\'utama\',new.kode_brg,new.stok);\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `ttblbarangau` AFTER UPDATE ON `tblbarang` FOR EACH ROW BEGIN\nif old.harga_beli!=new.harga_beli or old.Harga_jual!=new.harga_jual or old.Harga_jual2!=new.harga_jual2 or old.harga_jual3!=new.harga_jual3 then\ninsert into histori_harga values(\'\',curdate(),new.kode_brg,old.harga_beli,new.harga_beli,old.harga_jual,new.harga_jual,old.harga_jual2,new.harga_jual2,old.harga_jual3,new.harga_jual3);\nend if;\nEND'
sql_modes=1342177280 1342177280 1342177280
definers='root@localhost' 'root@localhost' 'root@localhost'
